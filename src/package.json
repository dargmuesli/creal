    "plyr": "3.8.4",
    "prettier": "3.8.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "pretty-bytes": "7.1.0",
    "serve": "14.2.5",
    "sharp": "0.34.5",
    "stylelint": "17.1.1",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "40.0.0",
    "stylelint-no-unsupported-browser-features": "8.1.1",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "4.1.18",
    "ufo": "1.6.3",
    "vue": "3.5.27",
