{
  "name": "creal",
  "version": "0.0.0",
  "description": "cReal's website.",
  "author": "Jonas Thelemann",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
