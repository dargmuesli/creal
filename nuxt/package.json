{
  "name": "creal",
  "private": true,
  "version": "1.7.0",
  "description": "DJ cReal's website.",
  "author": "Jonas Thelemann",
  "type": "module",
  "engines": {
    "node": "18"
  },
  "packageManager": "pnpm@8.0.0",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "cypress:open": "cypress open",
    "cypress:test": "./cypress/snapshots/test.sh",
    "test": "pnpm test:integration:dev",
    "test:integration:dev": "cross-env WAIT_ON_TIMEOUT=100000 start-server-and-test 'pnpm dev' 'http://localhost:3000' 'pnpm cypress:test'",
    "test:integration:prod": "cross-env WAIT_ON_TIMEOUT=10000 NODE_ENV=production start-server-and-test 'pnpm start' 'http://localhost:3000' 'pnpm cypress:test'",
    "test:integration:docker:build": "docker build -t test-integration_base --build-arg UID=$(id -u) --build-arg GID=$(id -g) --target test-integration_base ../",
    "test:integration:docker:run": "docker run --rm --entrypoint '' -v \"$PWD:/srv/app\" test-integration_base",
    "test:integration:docker:br": "pnpm test:integration:docker:build && pnpm test:integration:docker:run",
    "test:integration:docker:dev": "pnpm test:integration:docker:br pnpm test:integration:dev",
    "test:integration:docker:prod": "pnpm test:integration:docker:br pnpm test:integration:prod",
    "lint": "pnpm lint:js && pnpm lint:ts && pnpm lint:style",
    "lint:js": "eslint --cache --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:ts": "pnpm nuxt typecheck",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:fix": "pnpm lint:js --fix && pnpm lint:ts --fix && pnpm lint:style --fix",
    "lint:quiet": "pnpm lint:js --quiet && pnpm lint:style --quiet",
    "lint:staged": "pnpm lint-staged",
    "supportedBrowsers": "echo \"export default $(browserslist-useragent-regexp --allowHigherVersions);\" > supportedBrowsers.js",
    "gql:codegen": "NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --config .codegenrc.ts",
    "prepare": "pnpm nuxt prepare"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "3.301.0",
    "@aws-sdk/credential-providers": "3.301.0",
    "@aws-sdk/s3-request-presigner": "3.301.0",
    "@babel/core": "7.21.3",
    "@dargmuesli/nuxt-cookie-control": "5.2.2",
    "@funken-studio/sitemap-nuxt-3": "4.0.4",
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@graphql-codegen/typescript-urql-graphcache": "2.4.4",
    "@graphql-codegen/urql-introspection": "2.2.1",
    "@http-util/status-i18n": "0.6.2",
    "@intlify/eslint-plugin-vue-i18n": "2.0.0",
    "@nuxtjs/color-mode": "3.2.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@nuxtjs/html-validator": "1.2.4",
    "@nuxtjs/i18n": "npm:@nuxtjs/i18n-edge@8.0.0-beta.9-27948515.3db6367",
    "@nuxtjs/robots": "3.0.0",
    "@nuxtjs/sitemap": "2.4.0",
    "@pinia/nuxt": "0.4.7",
    "@rollup/plugin-graphql": "2.0.3",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/line-clamp": "0.4.2",
    "@tailwindcss/typography": "0.5.9",
    "@types/cookie": "0.5.1",
    "@types/lodash-es": "4.17.7",
    "@types/marked": "4.0.8",
    "@types/node": "18.15.11",
    "@unhead/vue": "1.1.25",
    "@urql/core": "3.2.2",
    "@urql/devtools": "2.0.3",
    "@urql/exchange-graphcache": "5.2.0",
    "@urql/vue": "1.0.5",
    "@vuelidate/core": "2.0.2",
    "@vuelidate/validators": "2.0.1",
    "autoprefixer": "10.4.14",
    "clipboard": "2.0.11",
    "consola": "2.15.3",
    "cookie": "0.5.0",
    "cross-env": "7.0.3",
    "cypress": "12.8.1",
    "cypress-visual-regression": "3.0.0",
    "dayjs": "2.0.0-alpha.4",
    "defu": "6.1.2",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-compat": "4.1.2",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-yml": "1.5.0",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "h3": "1.6.4",
    "is-https": "4.0.0",
    "jose": "4.13.1",
    "lint-staged": "13.2.0",
    "lodash-es": "4.17.21",
    "marked": "4.3.0",
    "moment": "2.29.4",
    "node-fetch": "3.3.1",
    "nuxt": "3.3.2",
    "nuxt-schema-org": "2.1.2",
    "pinia": "2.0.33",
    "postcss": "8.4.21",
    "postcss-html": "1.5.0",
    "prettier": "2.8.7",
    "prettier-plugin-tailwindcss": "0.2.5",
    "pretty-bytes": "6.1.0",
    "rollup": "3.20.2",
    "slugify": "1.6.6",
    "start-server-and-test": "2.0.0",
    "stylelint": "15.3.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "31.0.0",
    "stylelint-no-unsupported-browser-features": "6.1.0",
    "sweetalert2": "11.7.3",
    "tailwindcss": "3.2.7",
    "ts-node": "10.9.1",
    "typescript": "5.0.2",
    "vue": "3.2.47",
    "vue-gtag": "2.0.1",
    "vue-plyr": "7.0.0",
    "vue-tsc": "1.2.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "webpack"
      ]
    }
  }
}
