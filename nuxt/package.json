    "@nuxtjs/eslint-config-typescript": "1.0.2",
    "@nuxtjs/eslint-module": "1.2.0",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-plugin-nuxt": "1.0.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "prettier": "1.19.1",
    "husky": "4.2.5",
    "lint-staged": "10.2.7",
    "@nuxtjs/stylelint-module": "3.2.2",
    "stylelint": "10.1.0"
