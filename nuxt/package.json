{
  "name": "creal",
  "private": true,
  "version": "1.9.1",
  "description": "DJ cReal's website.",
  "author": "Jonas Thelemann",
  "type": "module",
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "start": "PORT=3001 node .output/server/index.mjs",
    "lint": "pnpm lint:js && pnpm lint:ts && pnpm lint:style",
    "lint:js": "eslint --cache --ext .js,.ts,.vue --ignore-path .gitignore .",
    "lint:ts": "nuxt typecheck",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:fix": "pnpm lint:js --fix && pnpm lint:ts --fix && pnpm lint:style --fix",
    "lint:quiet": "pnpm lint:js --quiet && pnpm lint:style --quiet",
    "lint:staged": "pnpm lint-staged",
    "prepare": "nuxt prepare",
    "test:e2e": "playwright test",
    "test:e2e:dev": "cross-env NUXT_PUBLIC_IS_TESTING=1 WAIT_ON_TIMEOUT=100000 pnpm test:e2e",
    "test:e2e:prod": "cross-env NUXT_PUBLIC_IS_TESTING=1 WAIT_ON_TIMEOUT=10000 NODE_ENV=production pnpm test:e2e",
    "test:e2e:docker:build": "docker build -t test-e2e_base --build-arg UID=$(id -u) --build-arg GID=$(id -g) --target test-e2e_base ..",
    "test:e2e:docker:run": "docker run --rm -v \"$PWD/..:/srv/app\" -v \"$(pnpm store path):/srv/.pnpm-store\" test-e2e_base",
    "test:e2e:docker:br": "pnpm test:e2e:docker:build && pnpm test:e2e:docker:run",
    "test:e2e:docker:dev": "pnpm test:e2e:docker:br pnpm --dir nuxt run test:e2e:dev",
    "test:e2e:docker:dev:update": "pnpm test:e2e:docker:br pnpm --dir nuxt run test:e2e:dev --update-snapshots",
    "test:e2e:docker:prod": "pnpm test:e2e:docker:br pnpm --dir nuxt run test:e2e:prod",
    "supportedBrowsers": "echo \"export default $(browserslist-useragent-regexp --allowHigherVersions);\" > supportedBrowsers.js",
    "gql:codegen": "NODE_TLS_REJECT_UNAUTHORIZED=0 graphql-codegen --config .codegenrc.ts"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "3.359.0",
    "@aws-sdk/credential-providers": "3.359.0",
    "@aws-sdk/s3-request-presigner": "3.359.0",
    "@axe-core/playwright": "4.7.3",
    "@babel/core": "7.22.5",
    "@dargmuesli/nuxt-cookie-control": "5.10.2",
    "@funken-studio/sitemap-nuxt-3": "4.0.4",
    "@graphql-codegen/cli": "4.0.1",
    "@graphql-codegen/client-preset": "4.0.1",
    "@graphql-codegen/typescript-urql-graphcache": "2.4.5",
    "@graphql-codegen/urql-introspection": "2.2.1",
    "@http-util/status-i18n": "0.7.0",
    "@intlify/eslint-plugin-vue-i18n": "2.0.0",
    "@nuxtjs/color-mode": "3.3.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@nuxtjs/html-validator": "1.4.0",
    "@nuxtjs/i18n": "8.0.0-beta.12",
    "@nuxtjs/robots": "3.0.0",
    "@pinia/nuxt": "0.4.11",
    "@playwright/test": "1.35.1",
    "@skjnldsv/vue-plyr": "7.3.1",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "@types/cookie": "0.5.1",
    "@types/lodash-es": "4.17.7",
    "@types/marked": "5.0.0",
    "@types/node": "20.1.7",
    "@unhead/vue": "1.1.27",
    "@urql/core": "4.0.10",
    "@urql/devtools": "2.0.3",
    "@urql/exchange-graphcache": "6.1.4",
    "@urql/vue": "1.1.2",
    "@vuelidate/core": "2.0.2",
    "@vuelidate/validators": "2.0.2",
    "autoprefixer": "10.4.14",
    "clipboard": "2.0.11",
    "consola": "3.1.0",
    "cookie": "0.5.0",
    "cross-env": "7.0.3",
    "dayjs": "2.0.0-alpha.4",
    "defu": "6.1.2",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-compat": "4.1.4",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-yml": "1.8.0",
    "h3": "1.7.0",
    "is-https": "4.0.0",
    "jose": "4.14.4",
    "lint-staged": "13.2.2",
    "lodash-es": "4.17.21",
    "marked": "5.1.0",
    "moment": "2.29.4",
    "nuxt": "3.6.0",
    "nuxt-schema-org": "2.2.0",
    "pinia": "2.1.4",
    "postcss": "8.4.24",
    "postcss-html": "1.5.0",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "pretty-bytes": "6.1.0",
    "rollup": "3.25.1",
    "slugify": "1.6.6",
    "stylelint": "15.9.0",
    "stylelint-config-recommended-vue": "1.4.0",
    "stylelint-config-standard": "33.0.0",
    "stylelint-no-unsupported-browser-features": "6.1.0",
    "sweetalert2": "11.7.12",
    "tailwindcss": "3.3.2",
    "ts-node": "10.9.1",
    "typescript": "5.1.3",
    "vue": "3.3.4",
    "vue-gtag": "2.0.1",
    "vue-tsc": "1.8.1"
  }
}
