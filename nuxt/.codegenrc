{
  "overwrite": true,
  "schema": "https://creal-postgraphile.localhost/graphql",
  "documents": "gql/**/*.gql",
  "generates": {
    "gql/schema.ts": {
      "plugins": [
        "typescript",
        "typescript-urql-graphcache"
      ]
    },
    "gql/generated.ts": {
      "plugins": [
        "typescript",
        "typescript-operations",
        "typescript-vue-urql"
      ]
    },
    "gql/introspection.ts": {
      "plugins": [
        "urql-introspection"
      ]
    }
  }
}
